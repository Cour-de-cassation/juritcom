{
  "name": "juritcom",
  "version": "0.0.1",
  "description": "JURITCOM",
  "author": "Cour de Cassation",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier \"src/**/*.ts\" --check",
    "start": "NODE_ENV=local nest start",
    "start:watch": "NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/api/main",
    "batch:start": "npx ts-node -r dotenv/config src/batch/normalization/index.ts",
    "batch:start:watch": "nodemon --config nodemon-batch.json",
    "batch:start:prod": "node -r dotenv/config dist/batch/normalization/index.js",
    "start:docker": "docker compose up -d",
    "stop:docker": "docker compose stop",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "test": "jest && npm run test:integration",
    "test:api": "jest --testPathPattern=src/api & npm run test:integration",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --passWithNoTests --config ./jest-integration.json --runInBand",
    "fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && prettier \"src/**/*.ts\" --write"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.651.0",
    "@nestjs/axios": "3.0.3",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/terminus": "10.2.3",
    "@node-oauth/express-oauth-server": "4.1.1",
    "@node-oauth/oauth2-server": "5.2.0",
    "axios": "1.12.1",
    "body-parser": "1.20.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cron": "3.1.7",
    "dbsder-api-types": "2.1.1",
    "express-basic-auth": "1.2.1",
    "html-entities": "2.5.2",
    "html-to-text": "9.0.5",
    "joi": "17.13.3",
    "jsdom": "26.1.0",
    "jsonwebtoken": "9.0.2",
    "jwks-rsa": "3.1.0",
    "marked": "15.0.7",
    "minio": "8.0.2",
    "nestjs-pino": "4.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-oauth2": "1.8.0",
    "pino-http": "10.3.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "10.1.4",
    "@nestjs/testing": "10.4.1",
    "@smithy/util-stream": "3.1.4",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.13",
    "@types/jsdom": "21.1.7",
    "@types/multer": "1.4.12",
    "@types/node": "22.5.4",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "aws-sdk-client-mock": "4.0.1",
    "aws-sdk-client-mock-jest": "4.0.1",
    "eslint": "9.34.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "pino-pretty": "11.2.2",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/.jest/setupEnvVars.ts"
    ]
  }
}
